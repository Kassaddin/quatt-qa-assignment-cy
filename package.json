{
  "name": "quatt-qa-assignment-cy",
  "version": "1.0.0",
  "description": "quatt assignment cypress-ed",
  "main": "index.js",
  "scripts": {
    "ui": "npx cypress open",
    "e2e": "cypress run -b chrome --headed --e2e -r mocha-junit-reporter --reporter-options mochaFile=reports/[hash].xml",
