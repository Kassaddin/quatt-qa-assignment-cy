    "type": "git",
    "url": "https://github.com/Kassaddin/quatt-qa-assignment-cy"
  },
  "keywords": [
    "quatt",
    "assignment",
    "pichkurov"
  ],
  "author": "Aleksandr Pichkurov",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cypress": "^13.13.2",
    "mocha-junit-reporter": "^2.2.1"
  }
}
